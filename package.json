{
  "name": "@mediamonks/muban-transition-component-legacy",
  "version": "5.0.0",
  "description": "Provides GreenSock transition functionality to Muban components.",
  "author": "frontend.monks",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "pre-push": [
    "validate"
  ],
  "keywords": [
    "seng",
    "mediamonks",
    "muban",
    "transition",
    "muban-transition-component"
  ],
  "bugs": {
    "url": "https://github.com/mediamonks/muban-transition-component/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mediamonks/muban-transition-component.git"
  },
  "scripts": {
    "prepublishOnly": "npm-run-all -s validate build",
    "validate": "npm-run-all -p lint test",
    "dev": "npm-run-all -p dev:*",
    "dev:babel": "babel ./src -x \".ts\" --out-dir ./ --watch",
    "dev:ts": "tsc --noEmit --allowJs --watch",
    "build": "npm-run-all -s clean build:*",
    "build:babel": "babel ./src -x \".ts\" -x \".js\" --out-dir ./",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "test": "cross-env NODE_ENV=test nyc mocha --require ./test/_setup/setup.js --require jsdom-global/register \"./test/**/*.spec.{ts,js}\"",
    "test:dev": "mocha -w --watch-extensions ts,js \"./test/**/*.spec.{ts,js}\"",
    "clean": "npm-run-all clean:*",
    "clean:test": "shx rm -rf coverage .nyc_output",
    "clean:npm": "shx rm -rf lib tmp index.js",
    "doc": "npm-run-all -p doc:*",
    "doc:typedoc": "typedoc --options typedoc.json",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src --ext .js,.ts --cache",
    "lint:ts": "tslint src/**/*.ts -c tslint.json -p tsconfig.json -t verbose",
    "prettify": "prettier --write \"src/**/*.{js,ts,json}\" \"tool/scaffold/src/**/*.{js,ts,json}\" \"tool/convert/src/**/*.{js,ts,json}\"",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-json-strings": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-stage-3": "^7.0.0-beta.35",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/register": "^7.0.0-beta.35",
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/sinon": "^4.1.2",
    "@types/sinon-chai": "^2.7.29",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^4.5.0",
    "coveralls": "^2.11.6",
    "cross-env": "^5.1.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-typescript": "^1.0.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^2.4.0",
    "gsap": "^3.13.0",
    "husky": "^0.14.3",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^6.0.0",
    "mocha": "^8.4.0",
    "muban-core": "^1.13.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^1.9.2",
    "scroll-tracker-component-manager": "^2.2.14",
    "shx": "^0.4.0",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.7.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.25.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.0",
    "@mediamonks/transition-controller": "^3.0.0",
    "bows": "^1.7.0",
    "confirm-simple": "^1.0.3",
    "lodash": "^4.17.5",
    "node-run-cmd": "^1.0.1",
    "seng-disposable": "^1.1.3",
    "seng-event": "^1.2.3",
    "transition-controller": "file:/Volumes/Proyects/transition-controller"
  },
  "peerDependencies": {
    "gsap": "^3.13.0",
    "muban-core": "^1.13.1"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,json}": [
        "prettier --write",
        "git add"
      ],
      "src/**/*.js": [
        "npm run lint:js"
      ],
      "src/**/*.ts": [
        "npm run lint:ts"
      ]
    }
  }
}
